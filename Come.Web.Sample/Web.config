    <add key="CollectiveOAuth_WECHAT_ENTERPRISE_ClientSecret" value="xxxxxxxxxxxxxxxxxxxxxxxxxxx" />
    <add key="CollectiveOAuth_WECHAT_ENTERPRISE_Scope" value="snsapi_userinfo" />
